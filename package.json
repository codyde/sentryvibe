{
  "name": "sentryvibe-monorepo",
  "version": "0.4.2",
  "description": "AI-powered project generation platform with Claude AI and remote runner architecture",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/codyde/sentryvibe.git"
  },
  "author": "Cody De Arkland",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm --filter sentryvibe dev",
    "build": "pnpm --filter sentryvibe build",
    "build:all": "pnpm --filter sentryvibe build && pnpm --filter @sentryvibe/runner-cli build && pnpm --filter sentryvibe-broker build",
    "build:cli": "pnpm --filter @sentryvibe/runner-cli build",
    "build:agent-core": "pnpm --filter @sentryvibe/agent-core build",
    "start": "pnpm --filter sentryvibe start",
    "lint": "pnpm --filter sentryvibe lint",
    "dev:all": "pnpm --filter sentryvibe dev & sleep 3 && pnpm --parallel --filter sentryvibe-broker --filter @sentryvibe/runner-cli run dev",
    "broker": "pnpm --filter sentryvibe-broker dev",
    "runner": "pnpm --filter @sentryvibe/runner-cli dev"
  },
  "pnpm": {
    "overrides": {
      "@sentry/core": "file:./apps/sentryvibe/vendor/sentry-core-LOCAL.tgz",
      "@sentry/node": "file:./apps/sentryvibe/vendor/sentry-node-LOCAL.tgz",
      "@sentry/node-core": "file:./apps/sentryvibe/vendor/sentry-node-core-LOCAL.tgz",
      "@sentry/nextjs": "file:./apps/sentryvibe/vendor/sentry-nextjs-LOCAL.tgz"
    }
  }
}
