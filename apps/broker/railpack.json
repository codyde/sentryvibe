{
  "$schema": "https://schema.railpack.com",
  "provider": "node",
  "steps": {
    "build-agent-core": {
      "commands": [
        "cd ../../packages/agent-core && pnpm install && pnpm run build"
      ]
    },
    "setup": {
      "inputs": ["$build-agent-core"],
      "commands": [
        "pnpm install"
      ]
    },
    "compile": {
      "inputs": ["$setup"],
      "commands": [
        "pnpm run build"
      ]
    }
  },
  "deploy": {
    "startCommand": "pnpm start",
    "inputs": ["$compile"]
  }
}
