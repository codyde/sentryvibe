# SentryVibe Web App Configuration
# Copy this file to .env.local and update values

# Database connection string
DATABASE_URL=postgresql://postgres:password@localhost:5432/sentryvibe

# Runner broker configuration
RUNNER_SHARED_SECRET=dev-secret
RUNNER_BROKER_URL=ws://localhost:4000/socket
RUNNER_BROKER_HTTP_URL=http://localhost:4000

# Workspace for generated projects
WORKSPACE_ROOT=/Users/yourname/sentryvibe-workspace

# Runner configuration
RUNNER_DEFAULT_ID=local
RUNNER_ID=local
