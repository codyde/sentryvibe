{
  "name": "@sentryvibe/runner-cli",
  "version": "0.15.1",
  "description": "SentryVibe Runner CLI - Execute AI-powered builds locally",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "sentryvibe-cli": "./dist/cli/index.js",
    "sentryvibe": "./dist/cli/index.js"
  },
  "files": [
    "dist/",
    "templates/",
    "vendor/*.tgz",
    "scripts/",
    "templates.json",
    ".npmrc"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "sentryvibe",
    "runner",
    "cli",
    "ai",
    "build",
    "claude"
  ],
  "author": "Cody De Arkland",
  "repository": {
    "type": "git",
    "url": "https://github.com/codyde/sentryvibe.git",
    "directory": "apps/runner"
  },
  "bugs": {
    "url": "https://github.com/codyde/sentryvibe/issues"
  },
  "homepage": "https://github.com/codyde/sentryvibe#readme",
  "preferGlobal": true,
  "scripts": {
    "postinstall": "node scripts/install-vendor.js",
    "dev": "tsx src/index.ts",
    "dev:cli": "tsx src/cli/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "cli": "node dist/cli/index.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.53",
    "@anthropic-ai/claude-agent-sdk": "^0.1.30",
    "@clack/prompts": "^0.11.0",
    "@openai/codex-sdk": "^0.53.0",
    "@sentry/core": "file:./vendor/sentry-core-LOCAL.tgz",
    "@sentry/nextjs": "file:./vendor/sentry-nextjs-LOCAL.tgz",
    "@sentry/node": "file:./vendor/sentry-node-LOCAL.tgz",
    "@sentry/node-core": "file:./vendor/sentry-node-core-LOCAL.tgz",
    "@sentryvibe/agent-core": "workspace:*",
    "ai": "^5.0.79",
    "ai-sdk-provider-claude-code": "file:./vendor/ai-sdk-provider-claude-code-LOCAL.tgz",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "conf": "^15.0.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.5",
    "ink": "^6.3.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^12.9.6",
    "ora": "^9.0.0",
    "pg": "^8.13.1",
    "picocolors": "^1.1.1",
    "react": "19.1.0",
    "simple-git": "^3.28.0",
    "update-notifier": "^7.3.1",
    "ws": "^8.18.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/update-notifier": "^6.0.8",
    "@types/ws": "^8.5.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
