{
  "name": "@sentryvibe/cli",
  "version": "0.31.4",
  "description": "SentryVibe CLI - AI-powered app builder",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "sentryvibe-cli": "./bin/sentryvibe.js",
    "sentryvibe": "./bin/sentryvibe.js"
  },
  "files": [
    "bin/",
    "dist/",
    "templates/",
    "vendor/*.tgz",
    "scripts/",
    "templates.json",
    ".npmrc"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "sentryvibe",
    "runner",
    "cli",
    "ai",
    "build",
    "claude"
  ],
  "author": "Cody De Arkland",
  "repository": {
    "type": "git",
    "url": "https://github.com/codyde/sentryvibe.git",
    "directory": "apps/runner"
  },
  "bugs": {
    "url": "https://github.com/codyde/sentryvibe/issues"
  },
  "homepage": "https://github.com/codyde/sentryvibe#readme",
  "preferGlobal": true,
  "scripts": {
    "postinstall": "node scripts/install-vendor.js",
    "dev": "RUNNER_WS_URL=ws://localhost:3000/ws/runner tsx --import ./src/instrument.ts src/index.ts",
    "dev:cli": "tsx --import ./src/instrument.ts src/cli/index.ts",
    "build": "rollup -c && cp templates.json dist/",
    "build:tsc": "tsc -p tsconfig.json",
    "start": "RUNNER_WS_URL=ws://localhost:3000/ws/runner node --import ./dist/instrument.js dist/index.js",
    "cli": "node --import ./dist/instrument.js dist/cli/index.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.53",
    "@anthropic-ai/claude-agent-sdk": "^0.2.12",
    "@clack/prompts": "^0.11.0",
    "@openai/codex-sdk": "^0.53.0",
    "@sentry/core": "file:./vendor/sentry-core-LOCAL.tgz",
    "@sentry/nextjs": "file:./vendor/sentry-nextjs-LOCAL.tgz",
    "@sentry/node": "file:./vendor/sentry-node-LOCAL.tgz",
    "@sentry/node-core": "file:./vendor/sentry-node-core-LOCAL.tgz",
    "@sentryvibe/agent-core": "workspace:*",
    "@sentryvibe/opencode-client": "workspace:*",
    "ai": "^5.0.79",
    "ai-sdk-provider-claude-code": "file:./vendor/ai-sdk-provider-claude-code-LOCAL.tgz",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "conf": "^15.0.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.5",
    "express": "^5.1.0",
    "http-proxy": "^1.18.1",
    "ink": "^6.3.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^12.9.6",
    "jsonc-parser": "^3.3.1",
    "ora": "^9.0.0",
    "pg": "^8.13.1",
    "picocolors": "^1.1.1",
    "react": "19.1.0",
    "simple-git": "^3.28.0",
    "ws": "^8.18.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/express": "^4.17.21",
    "@types/http-proxy": "^1.17.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/ws": "^8.5.10",
    "rollup": "^4.28.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
