    "conf": "^15.0.2",
    "dotenv": "^16.4.5",
    "inquirer": "^12.9.6",
    "ora": "^9.0.0",
    "simple-git": "^3.28.0",
    "update-notifier": "^7.3.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/update-notifier": "^6.0.8",
    "@types/ws": "^8.5.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
