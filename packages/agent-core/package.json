{
  "name": "@sentryvibe/agent-core",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./lib/agents": {
      "types": "./dist/lib/agents/index.d.ts",
      "default": "./dist/lib/agents/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./shared/runner/messages": {
      "types": "./dist/shared/runner/messages.d.ts",
      "default": "./dist/shared/runner/messages.js"
    },
    "./shared/*": {
      "types": "./dist/shared/*.d.ts",
      "default": "./dist/shared/*.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "default": "./dist/types/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "ai": "^5.0.57",
    "ai-sdk-provider-claude-code": "^1.1.4",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "lucide-react": "^0.544.0",
    "pg": "^8.13.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1"
  }
}
