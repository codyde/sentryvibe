{
  "name": "@sentryvibe/agent-core",
  "version": "0.17.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./lib/agents": {
      "types": "./dist/lib/agents/index.d.ts",
      "default": "./dist/lib/agents/index.js"
    },
    "./lib/runner/broker-state": {
      "types": "./dist/lib/runner/broker-state.d.ts",
      "default": "./dist/lib/runner/broker-state.js"
    },
    "./lib/runner/event-stream": {
      "types": "./dist/lib/runner/event-stream.d.ts",
      "default": "./dist/lib/runner/event-stream.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./shared/runner/messages": {
      "types": "./dist/shared/runner/messages.d.ts",
      "default": "./dist/shared/runner/messages.js"
    },
    "./shared/*": {
      "types": "./dist/shared/*.d.ts",
      "default": "./dist/shared/*.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "default": "./dist/types/*.js"
    },
    "./config/*": {
      "types": "./dist/config/*.d.ts",
      "default": "./dist/config/*.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.25",
    "@sentry/node": "file:../../apps/sentryvibe/vendor/sentry-node-LOCAL.tgz",
    "ai": "^5.0.57",
    "ai-sdk-provider-claude-code": "^2.1.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "lucide-react": "^0.544.0",
    "pg": "^8.13.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.0",
    "zod": "^3.25.76"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepare": "pnpm run build",
    "prepack": "pnpm run build"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "typescript": "^5.9.3"
  }
}
