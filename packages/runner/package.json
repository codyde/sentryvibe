{
  "name": "@openbuilder/runner",
  "version": "0.50.33",
  "description": "Lightweight OpenBuilder Runner - Connect to OpenBuilder server with minimal footprint",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "openbuilder-runner": "./bin/runner.js"
  },
  "files": [
    "bin/",
    "dist/",
    "vendor/*.tgz",
    "scripts/",
    ".npmrc"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "openbuilder",
    "ai",
    "claude",
    "runner",
    "lightweight"
  ],
  "author": "Cody De Arkland",
  "repository": {
    "type": "git",
    "url": "https://github.com/codyde/openbuilder.git",
    "directory": "packages/runner"
  },
  "bugs": {
    "url": "https://github.com/codyde/openbuilder/issues"
  },
  "homepage": "https://github.com/codyde/openbuilder#readme",
  "preferGlobal": true,
  "scripts": {
    "postinstall": "node scripts/install-vendor.js",
    "build": "rollup -c",
    "build:tsc": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@sentry/core": "file:./vendor/sentry-core-LOCAL.tgz",
    "@sentry/node": "file:./vendor/sentry-node-LOCAL.tgz",
    "@sentry/node-core": "file:./vendor/sentry-node-core-LOCAL.tgz",
    "ai-sdk-provider-claude-code": "file:./vendor/ai-sdk-provider-claude-code-LOCAL.tgz",
    "@ai-sdk/openai": "^2.0.53",
    "@anthropic-ai/claude-agent-sdk": "^0.2.12",
    "@openai/codex-sdk": "^0.53.0",
    "ai": "^5.0.79",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "conf": "^15.0.2",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "ink": "^6.3.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "picocolors": "^1.1.1",
    "react": "19.1.0",
    "ws": "^8.18.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@openbuilder/cli": "workspace:*",
    "@openbuilder/agent-core": "workspace:*",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/ws": "^8.5.10",
    "rollup": "^4.28.0",
    "typescript": "^5.6.3"
  }
